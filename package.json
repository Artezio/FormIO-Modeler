{
  "name": "formio-electron",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "dependencies": {
    "electron": "^7.1.9",
    "formiojs": "^4.8.0-rc.13",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "electron-packager": "^14.2.0"
  },
  "scripts": {
    "start": "electron main.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=data/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=data/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Formio\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=data/icons/png/icon.png --prune=true --out=release-builds"
  }
}